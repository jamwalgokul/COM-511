def Linear_search (arr,target): 
for i in range (lencarr)):
if arr[i] == target:
return i
return -1
def binary_search(arr_target):
left,right = 0, len(arr)-1
while left <= right:
mid = (left + right) // 2 
if arr[mid] == target:
return mid
elif arr[mid]< target:
left=mid+1
else:
    right=mid-1
return -1
mylist = [1,3,5,7,9, 14, 13).
